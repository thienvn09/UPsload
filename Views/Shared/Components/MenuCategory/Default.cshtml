@model IEnumerable<DoAn.Models.CategoryMenu>
<div class="col-lg-12">
    <div class="mb-3">
        <h4>Categories</h4>
        <ul class="list-unstyled fruite-categorie">
            @foreach (var item in Model)
            {
                <li>
                    <div class="d-flex justify-content-between fruite-name">
                        <a href="#"><i class="fas fa-apple-alt me2"></i>@item.Title</a>
                        <span>(@item.Count)</span>
                    </div>
                </li>
            }
        </ul>
    </div>
</div>